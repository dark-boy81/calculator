{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "/api/index.py",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "static/$1"
    }
  ]
}
