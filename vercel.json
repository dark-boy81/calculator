{
  "version": 2,
  "builds": [
    {
      "src": "bot.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/webhook/.*",
      "dest": "bot.py"
    },
    {
      "src": "/(.*)",
      "dest": "static/$1"
    }
  ]
}